  "scripts": {
    "start": "node ./index.js",
    "start-watch": "nodemon ./index.js --watch",
    "build": "node_modules/.bin/webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "node_modules/.bin/eslint ./src/js --fix --ext .js;",
    "lint-srv": "node_modules/.bin/eslint ./srv --fix --ext .js && node_modules/.bin/eslint index.js --fix --ext .js;",
    "create-30-db": "node -e 'require(\"./srv/manageJawsdb\").createJawsDBInstances(30)'",
    "create-1-db": "node -e 'require(\"./srv/manageJawsdb\").createJawsDBInstances(1)'",
    "list-dbs": "node -e 'require(\"./srv/manageJawsdb\").listJawsDBInstances()'",
    "delete-all-db": "node -e 'require(\"./srv/manageJawsdb\").deleteAllJawsDBInstances()'",
    "generate-public-json": "node -e 'require(\"./srv/manageJawsdb\").generatePublicJson()'",
    "generate-private-json": "node -e 'require(\"./srv/manageJawsdb\").generatePrivateJson()'",
    "remove-passwords": "node -e 'require(\"./srv/removePasswords\").removePasswords()'",
    "dumpAllDatabases": "node -e 'require(\"./srv/manageJawsdb\").dumpAllDatabases()'",
    "student-work-ts": "npx tsc --outDir ./srv/sql-grader/dist;",
    "student-work-check": "npm run student-work-ts; npx mocha ./srv/sql-grader/dist/run.test.js --reporter mochawesome --classname teacher",
    "all-student-work-check": "npm run student-work-ts; node ./srv/sql-grader/dist/all-student-work-check.js",
    "generate-pdfs": "python3 ./srv/excel-to-pdf-reporter/report.py --excel_file_path='./srv/excel-to-pdf-reporter/M290.23-24.PA-Noten.xlsx' "
  },
